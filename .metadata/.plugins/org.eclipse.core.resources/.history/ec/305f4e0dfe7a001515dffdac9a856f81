package com.anjali.springBasics.dependencyInjection;

public class Triangle implements IShape{
	int l;
	int b;
	
	public Triangle(int l, int b) {
		// TODO Auto-generated constructor stub
		this.l = l;
		this.b = b;
	}
	@Override
	public double area() {
		// TODO Auto-generated method stub
		return (l*b)/2;
	}

}
